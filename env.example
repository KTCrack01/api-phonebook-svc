DATABASE_USERNAME=noticeadmin
DATABASE_PASSWORD=KTcrack01

# ===========================================
# 선택적 환경변수 (기본값이 있어서 설정 안해도 됨)
# ===========================================

# JPA Configuration (기본값: update, true)
JPA_DDL_AUTO=update
JPA_SHOW_SQL=false

