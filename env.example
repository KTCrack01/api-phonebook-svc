# 환경변수 예시 파일
# 실제 사용시 .env 파일을 만들어서 사용하세요

# Database Configuration (Azure PostgreSQL)
DATABASE_URL=jdbc:postgresql://your-server.postgres.database.azure.com:5432/your-database?currentSchema=api_phonebook_svc&sslmode=require
DATABASE_USERNAME=your-username
DATABASE_PASSWORD=your-password

# JPA Configuration
JPA_DDL_AUTO=create-drop
JPA_SHOW_SQL=true

# Server Configuration  
SERVER_PORT=8081
